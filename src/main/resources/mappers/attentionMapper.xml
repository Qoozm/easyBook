<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xupt.dao.IAttentionDao">

    <insert id="save" parameterType="com.xupt.bean.AttentionExample">
        insert into ${tableName} values(#{from_user_id},#{to_user_id})
    </insert>

    <delete id="deleteOneByToUserId" parameterType="com.xupt.bean.AttentionExample">
        delete from ${tableName} where to_user_id = #{to_user_id} AND from_user_id = #{from_user_id}
    </delete>

</mapper>